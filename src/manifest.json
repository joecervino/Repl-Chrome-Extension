{
  "name": "Repl Chrome Extesnion",
  "version": "0.1.0",
  "options_page": "options.html",
  "permissions": ["tabs", "storage"],
  "web_accessible_resources": ["*.html"],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
